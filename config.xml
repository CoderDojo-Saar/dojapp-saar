<?xml version='1.0' encoding='utf-8'?>
<widget id="de.coderdojosaar.Dojapp" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>CoderDojo-Saar</name>
    <description>
        Die App des Coder-Dojo Saar
    </description>
    <author email="hello@coderdojo-saar.de" href="http://coderdojo-saar.de">
        Das Team der Dojapp
    </author>
    <content src="index.html" />
    <access origin="https://*.google.com/*" />
    <access origin="http://coderdojo-saar.de/*" />
    <plugin name="cordova-plugin-whitelist" spec="^1.3.3" />
    <plugin name="cordova-plugin-network-information" spec="^2.0.1" />
    <plugin name="cordova-plugin-calendar" spec="^5.0.0">
        <variable name="CALENDAR_USAGE_DESCRIPTION" value=" " />
    </plugin>
    <plugin name="cordova-plugin-dialogs" spec="^2.0.1" />
    <engine name="ios" spec="^4.5.4" />
    <engine name="android" spec="^7.1.0" />
    <engine name="browser" spec="^5.0.3" />
</widget>
